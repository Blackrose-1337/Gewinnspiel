{
    "name": "contacts-reminder",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "prettify": "prettier -w src/",
        "build": "vue-tsc --noEmit && vite build",
        "preview": "vite preview --port 5050",
        "test:unit": "vitest --environment jsdom",
        "test": "vitest --run --coverage",
        "typecheck": "vue-tsc --noEmit && vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
    },
    "dependencies": {
        "@editorjs/editorjs": "^2.26.4",
        "@editorjs/header": "^2.7.0",
        "@editorjs/list": "^1.8.0",
        "@editorjs/paragraph": "^2.9.0",
        "@editorjs/quote": "^2.5.0",
        "@editorjs/underline": "^1.1.0",
        "@quasar/extras": "^1.13.5",
        "@sotaproject/strikethrough": "^1.0.1",
        "date-fns": "^2.28.0",
        "editorjs-header-with-alignment": "^1.0.1",
        "editorjs-text-color-plugin": "^2.0.2",
        "ky": "^0.30.0",
        "lodash": "^4.17.21",
        "pinia": "^2.0.13",
        "quasar": "^2.6.6",
        "vue": "^3.2.33",
        "vue-loading-overlay": "^6.0.2",
        "vue-router": "^4.0.14"
    },
    "devDependencies": {
        "@quasar/vite-plugin": "^1.0.9",
        "@rushstack/eslint-patch": "^1.1.3",
        "@types/jsdom": "^16.2.14",
        "@types/lodash": "^4.14.182",
        "@types/node": "^17.0.26",
        "@vitejs/plugin-vue": "^2.3.1",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^10.0.0",
        "@vue/test-utils": "^2.0.0-rc.21",
        "@vue/tsconfig": "^0.1.3",
        "c8": "^7.11.2",
        "editorjs-inline-font-family-tool": "^1.0.4",
        "editorjs-inline-font-size-tool": "^1.0.1",
        "editorjs-text-alignment-blocktune": "^1.0.3",
        "eslint": "^8.14.0",
        "eslint-plugin-vue": "^8.7.1",
        "jsdom": "^19.0.0",
        "prettier": "^2.6.2",
        "sass": "1.32.0",
        "typescript": "~4.6.3",
        "vite": "^2.9.5",
        "vitest": "^0.9.4",
        "vue-tsc": "^0.34.10"
    }
